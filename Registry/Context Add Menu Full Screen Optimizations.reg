@=""
"MUIVerb"="Enable Full Screen Optimizations (Default)"
"icon"=""

[HKEY_CLASSES_ROOT\exefile\shell\FSO\Shell\EnableFSO\command]
@="\"C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe\" \"-File\" \"C:\\Windows\\Scripts\\Context.ps1\" \"%1\" \"EnableFSO\""