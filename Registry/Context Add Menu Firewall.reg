Windows Registry Editor Version 5.00

[HKEY_CLASSES_ROOT\exefile\shell\WindowsFirewall]
@=""
"MUIVerb"="Windows Firewall"
"icon"="%SystemRoot%\\system32\\FirewallControlPanel.dll,0"
"subcommands"=""

[HKEY_CLASSES_ROOT\exefile\shell\WindowsFirewall\Shell]
@=""



[HKEY_CLASSES_ROOT\exefile\shell\WindowsFirewall\Shell\AllowOutboundInternetTCPPort80]
@=""
"MUIVerb"="Allow Outbound Internet (TCP Port 80)"
"icon"="%SystemRoot%\\system32\\FirewallControlPanel.dll,2"

[HKEY_CLASSES_ROOT\exefile\shell\WindowsFirewall\Shell\AllowOutboundInternetTCPPort80\command]
@="\"C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe\" \"-File\" \"C:\\Windows\\Scripts\\Context.ps1\" \"%1\" \"AllowOutboundInternetTCPPort80\""


